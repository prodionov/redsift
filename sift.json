{
  "name": "email3",
  "display-name": "Email3 Sift",
  "description": "technical task",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "email": {
      "thread": {
        "controller": "dist/js/controller.umd-es2015.min.js"
      },
      "client": {
        "controller": "dist/js/email-client-controller.umd-es2015.min.js"
      }
    },
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs": {
      "emails": {
        "gmailEmails": {
          "filter": {
            "conditions": [
              {
                "date": "between now and 1 week before now"
              }
            ]
          },
          "wants": ["archive", "textBody", "strippedHtmlBody", "headers"],
          "inMailbox": "inbox"
        }
      },
      "slack-bot": {},
      "webhooks": {},
      "rpc": {}
    },
    "nodes": [
      {
        "#": "First Node",
        "input": {
          "bucket": "gmailEmails"
        },
        "implementation": {
          "javascript": "server/node1.js"
        },
        "outputs": {
          "count": {}
        }
      }
    ],
    "stores": {
      "output1": {}
    },
    "outputs": {
      "exports": {
        "count": {
          "key$schema": "string"
        }
      }
    }
  }
}
